<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.FizzleBizzle</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>  
   <div class="jumbotron top">
        <h1>Fizzle Bizzle Generator</h1>
        <hr width="50%" class="alert-info" />
        <h5>*now with Bazz!</h5>
    </div>
    <div id="Rounded" class="container body-content alert-info">
        @RenderBody()
    </div>
    <div id="Footer">
        <p>&copy; @DateTime.Now.Year - FizzleBizzle by Corey Davis</p>
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    
    @RenderSection("scripts", required: false)  
</body>
</html>
